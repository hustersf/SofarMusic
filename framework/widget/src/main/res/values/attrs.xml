<?xml version="1.0" encoding="utf-8"?>
<resources>

  <!--音乐进度控件-->
  <declare-styleable name="DoubleMusicProgress">
    <attr name="fReachColor" format="color"></attr>
    <attr name="sReachColor" format="color"></attr>
    <attr name="dUnreachColor" format="color"></attr>
    <attr name="circleColor" format="color"></attr>
    <attr name="fReachHeight" format="dimension"></attr>
    <attr name="sReachHeight" format="dimension"></attr>
    <attr name="dUnreachHeight" format="dimension"></attr>
    <attr name="radius" format="dimension"></attr>
  </declare-styleable>


  <!--音乐进度控件-->
  <declare-styleable name="MusicProgress">
    <attr name="reachColor" format="color"></attr>
    <attr name="unreachColor" format="color"></attr>
    <attr name="reachHeight" format="dimension"></attr>
    <attr name="unreachHeight" format="dimension"></attr>
    <attr name="roundconner" format="boolean"></attr>
  </declare-styleable>

  <!--仿TabLayout控件-->
  <declare-styleable name="SofarTabLayout">
    <attr name="tabIndicatorHeight" format="dimension"></attr>
    <attr name="tabIndicatorColor" format="color"></attr>
    <attr name="tabPadding" format="dimension"></attr>
    <attr name="tabPaddingStart" format="dimension"></attr>
    <attr name="tabPaddingTop" format="dimension"></attr>
    <attr name="tabPaddingEnd" format="dimension"></attr>
    <attr name="tabPaddingBottom" format="dimension"></attr>
  </declare-styleable>

  <!--手势滑动控件-->
  <declare-styleable name="SwipeLayout">
    <attr name="direction" format="string" />
    <attr name="fromEdge" format="boolean" />
  </declare-styleable>

  <!--圆角/圆形图片-->
  <declare-styleable name="RoundImageView">
    <attr name="cornerRadius" format="dimension"></attr>
    <attr name="cornerTopLeftRadius" format="dimension"></attr>
    <attr name="cornerTopRightRadius" format="dimension"></attr>
    <attr name="cornerBottomLeftRadius" format="dimension"></attr>
    <attr name="cornerBottomRightRadius" format="dimension"></attr>
    <attr name="borderWidth" format="dimension"></attr>
    <attr name="borderColor" format="color"></attr>
    <attr name="circle" format="boolean"></attr>
    <attr name="maskColor" format="color"></attr>
  </declare-styleable>

  <declare-styleable name="RoundRelativeLayout">
    <attr name="relativeLayoutRadius" format="dimension"></attr>
  </declare-styleable>

</resources>
